{
  "name": "@cycosoft/eyas",
  "version": "1.0.0",
  "description": "A \"shift-left\" serverless testing solution for static web applications",
  "main": "./index.js",
  "packageName": "Eyas",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "eyas": "./dist/cli/index.js"
  },
  "scripts": {
    "_______________DEV_______________": "",
    "dev:cli": "npm i && npm-check -u && npm run compile && node src/cli/index.js",
    "dev:eyas": "npm i && npm-check -u && electron src/main/index.js",
    "_____________COMPILE_____________": "",
    "compile": "node src/scripts/compile-dist.js && electron-vite build",
    "_____________SCRIPTS_____________": "",
    "bundle": "npm ci && npm run compile && npm pack"
  },
  "author": "Cycosoft, LLC (https://cycosoft.com)",
  "homepage": "https://cycosoft.com",
  "license": "GPL-3.0",
  "keywords": [
    "testing",
    "static",
    "web",
    "applications",
    "serverless"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "bytenode": "^1.5.1",
    "commander": "^11.0.0",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.10",
    "mkcert": "^3.0.0",
    "url-parse": "^1.5.10",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "electron-vite": "^1.0.27",
    "eslint": "^8.48.0",
    "npm-check": "^6.0.1"
  },
  "overrides": {
    "package-json": ">6.5.0 <8.0.0"
  }
}
