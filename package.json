{
  "name": "@cycosoft/eyas",
  "version": "1.1.0",
  "description": "Bundle your web code into an executable for fast serverless testing",
  "main": "./index.js",
  "packageName": "Eyas",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "eyas": "./dist/cli/index.js"
  },
  "scripts": {
    "_______________DEV_______________": "",
    "dev": "npm i && npm-check -u -i inquirer && npm run compile:module && node dist/cli/index.js",
    "dev:ui": "vite --config src/eyas-interface/app/.vite.config.ui.js --open",
    "_____________COMPILE_____________": "",
    "compile:module": "npm run compile:ui && node src/scripts/compile-module.js && electron-vite build",
    "compile:ui": "vite build --config src/eyas-interface/app/.vite.config.ui.js",
    "_____________SCRIPTS_____________": "",
    "bundle": "npm ci && npm run compile:module && npm pack",
    "deploy": "npm ci && npm run compile:module && npm publish",
    "bump:patch": "npm version patch && git push && git push --tags",
    "bump:minor": "npm version minor && git push && git push --tags"
  },
  "author": "Cycosoft, LLC (https://cycosoft.com)",
  "homepage": "https://cycosoft.com",
  "license": "MIT",
  "keywords": [
    "testing",
    "static",
    "web",
    "applications",
    "serverless",
    "eyas"
  ],
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "archiver": "^6.0.1",
    "bytenode": "^1.5.1",
    "commander": "^11.0.0",
    "date-fns": "^2.30.0",
    "electron": "^26.3.0",
    "electron-builder": "^24.6.4",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "inquirer": "8.2.6",
    "mixpanel": "^0.18.0",
    "mkcert": "^3.0.0",
    "node": "^20.7.0",
    "url-parse": "^1.5.10",
    "validator": "^13.11.0",
    "vue": "^3.3.4",
    "vuetify": "^3.3.20"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "electron-vite": "^1.0.28",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-check": "^6.0.1",
    "sass": "^1.69.0",
    "vite": "^4.4.11",
    "vite-plugin-vuetify": "^1.0.2"
  },
  "overrides": {
    "package-json": ">6.5.0 <8.0.0"
  }
}
