{
  "name": "@cycosoft/eyas",
  "version": "1.0.0",
  "description": "A \"shift-left\" serverless testing solution for static web applications",
  "main": "./.eyas-preview/index.js",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "eyas": "./dist/cli.js"
  },
  "build": {
    "appId": "com.cycosoft.eyas",
    "productName": "Eyas",
    "copyright": "Copyright Â© 2023 Cycosoft, LLC",
    "target": "portable",
    "compression": "store",
    "directories": {
      "output": ".eyas-dist",
      "buildResources": ".eyas-preview"
    },
    "removePackageScripts": true,
    "removePackageKeywords": true,
    "publish": null,
    "extraResources": [
      {
          "from": "dist/main",
          "to": ".eyas-preview"
      }
    ],
    "files": [
      "**/*"
    ]
  },
  "scripts": {
    "_______________DEV_______________": "",
    "dev:cli": "npm i && npm-check -u && npm run compile:eyas && node src/cli/index.js",
    "dev:eyas": "npm i && npm-check -u && electron src/main/index.js",
    "_____________COMPILE_____________": "",
    "compile:all": "npm run compile:eyas && npm run compile:cli",
    "compile:cli": "node src/cli/compile.js",
    "compile:eyas": "electron-vite build",
    "_____________SCRIPTS_____________": "",
    "bundle": "npm ci && npm run dist:clear && npm run compile:all && npm pack",
    "package": "electron-builder build",
    "start": "electron-builder start",
    "dist:clear": "shx rm -rf dist && shx mkdir dist"
  },
  "author": "Cycosoft, LLC (https://cycosoft.com)",
  "homepage": "https://cycosoft.com",
  "license": "GPL-3.0",
  "keywords": [
    "testing",
    "static",
    "web",
    "applications",
    "serverless"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@electron-forge/core": "^6.4.1",
    "bytenode": "^1.5.1",
    "commander": "^11.0.0",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.3",
    "electron-packager": "^17.1.2",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.10",
    "mkcert": "^3.0.0",
    "pkg": "^5.8.1",
    "url-parse": "^1.5.10",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "electron-vite": "^1.0.27",
    "eslint": "^8.48.0",
    "npm-check": "^6.0.1",
    "shx": "^0.3.4"
  },
  "overrides": {
    "package-json": ">6.5.0 <8.0.0"
  }
}
