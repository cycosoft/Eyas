{
  "name": "@cycosoft/eyas",
  "version": "1.0.0-rc.0",
  "description": "A no-code serverless visual testing solution for static web applications",
  "main": "./index.js",
  "packageName": "Eyas",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "eyas": "./dist/cli/index.js"
  },
  "scripts": {
    "_______________DEV_______________": "",
    "dev": "npm i && npm-check -u -i inquirer && npm run compile && node dist/cli/index.js",
    "_____________COMPILE_____________": "",
    "compile": "node src/scripts/compile-module.js && electron-vite build",
    "_____________SCRIPTS_____________": "",
    "bundle": "npm ci && npm run compile && npm pack"
  },
  "author": "Cycosoft, LLC (https://cycosoft.com)",
  "homepage": "https://cycosoft.com",
  "license": "MIT",
  "keywords": [
    "testing",
    "static",
    "web",
    "applications",
    "serverless",
    "eyas"
  ],
  "files": [
    "dist",
    "LICENSE.TXT"
  ],
  "dependencies": {
    "bytenode": "^1.5.1",
    "commander": "^11.0.0",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "inquirer": "8.2.6",
    "mkcert": "^3.0.0",
    "url-parse": "^1.5.10",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.49.0",
    "npm-check": "^6.0.1"
  },
  "overrides": {
    "package-json": ">6.5.0 <8.0.0"
  }
}
