{
  "name": "@cycosoft/eyas",
  "version": "0.1.0",
  "description": "A \"shift-left\" testing solution for static web applications",
  "publishConfig": {
    "access": "public"
  },
  "main": "eyas.min.js",
  "bin": {
    "eyas": "./cli.min.js"
  },
  "scripts": {
    "demo": "electron .",
    "dev": "npm i && npm-check -u && electron src/eyas.js",
    "build": "npm run build:eyas && npm run build:cli",
    "bundle": "npm ci && npm run build && npm pack",
    "_____________SCRIPTS_____________": "",
    "build:eyas": "uglifyjs src/eyas.js -c -m --toplevel --output eyas.min.js",
    "build:cli": "uglifyjs src/cli.js -c -m --toplevel -o cli.min.js"
  },
  "author": "Cycosoft, LLC (https://cycosoft.com)",
  "homepage": "https://cycosoft.com",
  "license": "GPL-3.0",
  "keywords": [
    "testing",
    "static",
    "web",
    "applications",
    "server"
  ],
  "files": [
    "cli.min.js",
    "eyas.min.js",
    "README.md"
  ],
  "dependencies": {
    "electron": "^26.0.0",
    "express": "^4.18.2",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^2.0.6",
    "mkcert": "^1.5.1"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "npm-check": "^6.0.1",
    "uglify-js": "^3.17.4"
  }
}
