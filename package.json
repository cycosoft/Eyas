{
  "name": "@cycosoft/eyas",
  "version": "0.1.0",
  "description": "A \"shift-left\" testing solution for static web applications",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "eyas": "./eyas-dist.js"
  },
  "scripts": {
    "start": "electron ./eyas-dist.js",
    "dev": "npm i && npm-check -u && electron src/index.js",
    "bundle": "npm ci && webpack && npm pack --dry-run"
  },
  "author": "Cycosoft, LLC (https://cycosoft.com)",
  "homepage": "https://cycosoft.com",
  "license": "GPL-3.0",
  "keywords": [
    "testing",
    "static",
    "web",
    "applications",
    "server"
  ],
  "files": [
    "eyas-dist.js",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "npm-check": "^6.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "electron": "^26.0.0",
    "express": "^4.18.2",
    "mkcert": "^1.5.1"
  }
}
